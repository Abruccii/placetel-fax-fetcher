{
  "name": "placetel-fax-fetcher",
  "version": "1.0.0",
  "description": "Checks Placetel API for new Fax and stores them in Filestorage",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "pkg ."
  },
  "pkg": {
    "assets": [
      "node_modules/**/*"
    ],
    "outputPath": "dist",
    "targets": [
      "node18-win-x64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abruccii/placetel-fax-fetcher.git"
  },
  "keywords": [
    "Placetel",
    "Fax",
    "Fetcher"
  ],
  "author": "abrucci",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Abruccii/placetel-fax-fetcher/issues"
  },
  "homepage": "https://github.com/Abruccii/placetel-fax-fetcher#readme",
  "dependencies": {
    "cli-progress": "^3.12.0",
    "fs-extra": "^11.2.0"
  }
}
